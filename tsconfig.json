{
    "compilerOptions": {
        "module": "ES6",
        "target": "ES6",
        "moduleResolution": "node",
        "baseUrl": ".",
        "removeComments": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
        "skipLibCheck": true
    }
}
